<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="horizontal" >

    <TextView
        android:id="@+id/tv_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:textSize="12sp"
        android:width="15dp" />

    <TextView
        android:id="@+id/tv_bssid"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:textSize="12sp"
        android:width="90dp" />

    <TextView
        android:id="@+id/tv_rssiMean"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:textSize="12sp"
        android:width="30dp" />

    <TextView
        android:id="@+id/tv_rssiVar"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:textSize="12sp"
        android:width="100dp" />

    <TextView
        android:id="@+id/tv_1RSS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:textSize="12sp"
        android:width="20dp" />

    <TextView
        android:id="@+id/tv_2RSS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:textSize="12sp"
        android:width="20dp" />

    <TextView
        android:id="@+id/tv_3RSS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:textSize="12sp"
        android:width="20dp" />

    <TextView
        android:id="@+id/tv_4RSS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:textSize="12sp"
        android:width="20dp" />

    <TextView
        android:id="@+id/tv_5RSS"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textIsSelectable="true"
        android:textSize="12sp"
        android:width="20dp" />

</LinearLayout>